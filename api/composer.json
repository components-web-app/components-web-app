{
    "name": "components-web-app/component-web-app-api",
    "description": "",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/silverbackdan/core"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "guzzlehttp/guzzle": "^6.3",
        "league/flysystem": "^2",
        "components-web-app/api-components-bundle": "dev-main",
        "symfony/dotenv": "^5.4 || ^6",
        "symfony/console": "^5.4 || ^6",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^5.4 || ^6",
        "symfony/mercure-bundle": "^0.3.2",
        "symfony/monolog-bundle": "^3.5",
        "symfony/yaml": "^5.4 || ^6"
    },
    "require-dev": {
        "api-platform/schema-generator": "^2.1",
        "behat/behat": "^3.6",
        "behatch/contexts": "^3.3",
        "friends-of-behat/mink": "^1.9",
        "friends-of-behat/mink-browserkit-driver": "^1.5",
        "friends-of-behat/mink-extension": "^2.5",
        "friends-of-behat/symfony-extension": "^2.1",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^5.4 || ^6",
        "symfony/css-selector": "^5.4 || ^6",
        "symfony/debug-bundle": "^5.4 || ^6",
        "symfony/maker-bundle": "^1.11",
        "symfony/phpunit-bridge": "^5.4 || ^6",
        "symfony/stopwatch": "^5.4 || ^6",
        "symfony/web-profiler-bundle": "^5.4 || ^6"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\Features\\Bootstrap\\" : "features/bootstrap/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
